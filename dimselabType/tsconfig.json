{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es6",
    "jsx": "react",
    "allowJs": true,
    "lib": [
      "dom",
      "es7"
    ],
    "baseUrl": "./src/js",
    "paths": {
      "@common/*": ["app/common/*"],
      "@interface/*": ["app/interface/*"],
      "@component/*": ["app/component/*"]
    }
  }
}